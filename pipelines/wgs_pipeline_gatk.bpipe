// GATK-based implementation of a WGS pipeline

about title: "WGS pipeline using BWA+GATK"

inputs "fastq.gz" : "Expecting compressed fastq files"

load 'wgs_pipeline_gatk.config'

// 

run {  "%_%_%_%_R*.fastq.gz" * [
        pipeline_prepare + trimmomatic + [
                fastqc.using(paired:"true"),
                bwa_pipeline + gatk_wgs_pipeline
	]
] }

